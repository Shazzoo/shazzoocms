{
    "name": "shazzoo/shazzoo_cms",
    "description": "A nice block based editor made with Laravel Livewire.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shazzoo\\Shazzoo_cms\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yazan sweed"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0|^11.0",
        "livewire/livewire": "^3.4"
    },

        "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.4",
        "spatie/laravel-ray": "^1.28",
        "spatie/x-ray": "^1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --provider='Shazzoo\\Shazzoo_cms\\CMSServiceProvider' --tag=config"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider='Shazzoo\\Shazzoo_cms\\CMSServiceProvider' --tag=config"
        ]
    },
         "config": {
            "sort-packages": true,
            "allow-plugins": {
                "pestphp/pest-plugin": true,
                "phpstan/extension-installer": true
            }
        },

        "extra": {
            "laravel": {
                "providers": [
                    "Shazzoo\\Shazzoo_cms\\CMSServiceProvider"
                ]
            }
        },



        "minimum-stability": "dev",
        "prefer-stable": true

}
